import java.time.LocalDateTime;
import java.time.format.DateTimeFormatter;

class Main {
    public static void main(String[] args) {
        LocalDateTime myobj = LocalDateTime.now();

        // Provide a proper pattern here
        DateTimeFormatter myfo = DateTimeFormatter.ofPattern("dd-MM-yyyy HH:mm:ss");

        String format = myobj.format(myfo);
        System.out.println("Date and time is: " + format);
    }
}